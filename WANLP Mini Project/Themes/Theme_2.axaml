<ResourceDictionary xmlns="https://github.com/avaloniaui"
                    xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml">
	<!--les couleur des themes-->
	<SolidColorBrush x:Key="couleur_principale_color" Color="#B6AD90"/>
	<SolidColorBrush x:Key="couleur_scondaire_color" Color="#A68A64"/>
	<SolidColorBrush x:Key="couleur_traissier_color" Color="#936639"/>
	<SolidColorBrush x:Key="couleur_quatrieme_color" Color="#7F4F24"/>

	<Color x:Key="color_principale_color">#B6AD90</Color>
	<Color x:Key="color_scondaire_color">#A68A64</Color>
	<Color x:Key="color_traissier_color">#936639</Color>
	<Color x:Key="color_quatrieme_color">#7F4F24</Color>

	<!-- parametre theme -->
	<Thickness x:Key="theme_1_thickness">0</Thickness>
	<Thickness x:Key="theme_2_thickness">2</Thickness>
	<Thickness x:Key="theme_3_thickness">0</Thickness>
	<Thickness x:Key="theme_4_thickness">0</Thickness>
	<Thickness x:Key="theme_5_thickness">0</Thickness>
	<Thickness x:Key="theme_6_thickness">0</Thickness>
</ResourceDictionary>
