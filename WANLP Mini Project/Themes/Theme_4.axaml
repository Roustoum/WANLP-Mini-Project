<ResourceDictionary xmlns="https://github.com/avaloniaui"
                    xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml">
	<!--les couleur des themes-->
	<SolidColorBrush x:Key="couleur_principale_color" Color="#5D3891"/>
	<SolidColorBrush x:Key="couleur_scondaire_color" Color="#7752FE"/>
	<SolidColorBrush x:Key="couleur_traissier_color" Color="#8E8FFA"/>
	<SolidColorBrush x:Key="couleur_quatrieme_color" Color="#C2D9FF"/>

	<Color x:Key="color_principale_color">#5D3891</Color>
	<Color x:Key="color_scondaire_color">#7752FE</Color>
	<Color x:Key="color_traissier_color">#8E8FFA</Color>
	<Color x:Key="color_quatrieme_color">#C2D9FF</Color>

	<!-- parametre theme -->
	<Thickness x:Key="theme_1_thickness">0</Thickness>
	<Thickness x:Key="theme_2_thickness">0</Thickness>
	<Thickness x:Key="theme_3_thickness">0</Thickness>
	<Thickness x:Key="theme_4_thickness">2</Thickness>
	<Thickness x:Key="theme_5_thickness">0</Thickness>
	<Thickness x:Key="theme_6_thickness">0</Thickness>
</ResourceDictionary>
